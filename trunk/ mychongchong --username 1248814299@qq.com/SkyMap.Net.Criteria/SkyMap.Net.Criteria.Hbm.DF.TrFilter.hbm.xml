<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" default-lazy="false">
	<class name="SkyMap.Net.Criteria.TrFilter,SkyMap.Net.Criteria" table="TR_Filter">

		<id name="Id" column="ID" type="String" unsaved-value="null">
			<generator class="assigned"/>
		</id>
		<many-to-one name="TyQuery" column="Query_ID" class="SkyMap.Net.Criteria.TyQuery,SkyMap.Net.Criteria" />
		<property column="Name" type="String" name="Name" not-null="true" length="50" />
		<property column="Description" type="String" name="Description" length="50" />
		<bag name="TrFilterConditions" lazy="false" cascade="delete" inverse="false" generic="true" outer-join="true" fetch="subselect">
			<key column="Filter_Id" />
			<one-to-many class="SkyMap.Net.Criteria.TrFilterCondition,SkyMap.Net.Criteria" />
		</bag>
		
	</class>
</hibernate-mapping>
