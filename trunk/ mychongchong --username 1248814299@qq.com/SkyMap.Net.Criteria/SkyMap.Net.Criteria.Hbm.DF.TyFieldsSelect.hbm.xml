<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" default-lazy="false">
	<class name="SkyMap.Net.Criteria.TyFieldsSelect,SkyMap.Net.Criteria" table="TY_FieldsSelect">

