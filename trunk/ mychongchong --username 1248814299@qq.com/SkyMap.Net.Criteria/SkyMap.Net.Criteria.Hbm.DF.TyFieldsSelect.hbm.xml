		<property column="Name" type="String" name="Name" length="50" />
		<property column="Field_Type" type="String" name="Type" length="50" />
		<property column="Description" type="String" name="Description" length="50" />
		<property column="Is_Display" type="String" name="IsDisplay" length="50" />
		<property column="Display_Name" type="String" name="DisplayName" length="50" />
		<property column="Display_Index" name="DisplayIndex" />
		<property column="Is_Condtion" type="String" name="IsCondtion" length="50" />
		<property column="Condition_Name" type="String" name="ConditionName" length="200" />
		<property column="TABLE_NAME" type="String" name="TableName" length="50" />
		<bag name="TyFilterConditions" lazy="false" cascade="delete" inverse="false" generic="true" outer-join="true" fetch="subselect">
			<key column="Field_Id" />
			<one-to-many class="SkyMap.Net.Criteria.TyFilterCondition,SkyMap.Net.Criteria" />
		</bag>
		
	</class>
</hibernate-mapping>
