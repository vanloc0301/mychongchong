<?xml version="1.0" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" default-lazy="false">
	<class name="SkyMap.Net.Workflow.XPDL.Application,SkyMap.Net.Workflow" table="WF_APPLICATION">
		<id name="Id" type="String" column="APPLICATION_ID">
			<generator class="assigned" />
		</id>
		<!--<version 
			name="ReplicationVersion" 
			column="REPLICATION_VERSION"
		/>-->
		<property name="Name" column="APPLICATION_NAME" />
		<property name="Description" column="APPLICATION_DESCRIPTION" />
		<property name="Assembly" column="APPLICATION_ASSEMBLY" />
		<property name="Procedure" column="APPLICATION_PROCEDURE" />
		<property name="TypeClass" column="APPLICATION_TYPECLASS" />
		<property name="ExecutionType" column="APPLICATION_EXECUTIONTYPE" />
		
	</class>
</hibernate-mapping>

