<?xml version="1.0" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" default-lazy="false">
	<class name="SkyMap.Net.Workflow.XPDL.Condition,SkyMap.Net.Workflow" table="WF_CONDITION">
		<id name="Id" type="String" column="CONDITION_ID">
			<generator class="assigned" />
		</id>
		<!--<version 
			name="ReplicationVersion" 
			column="REPLICATION_VERSION"
		/>-->
		<property name="Name" column="CONDITION_NAME" />
		<property name="Description" column="CONDITION_DESCRIPTION" />
		<property name="Type" column="CONDITION_TYPE" />
		<property name="Xpression" column="CONDITION_XPRESSION" />
		<many-to-one name="Application" column="APPLICATION_ID" class="SkyMap.Net.Workflow.XPDL.Application,SkyMap.Net.Workflow" cascade="none"
			/>
		
	</class>
</hibernate-mapping>

