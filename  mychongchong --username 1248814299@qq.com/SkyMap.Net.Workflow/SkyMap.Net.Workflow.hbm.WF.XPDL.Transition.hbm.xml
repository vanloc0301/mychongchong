<?xml version="1.0" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" default-lazy="false">
	<class name="SkyMap.Net.Workflow.XPDL.Transition,SkyMap.Net.Workflow" table="WF_TRANSITION">
		<id name="Id" type="String" column="TRANSITION_ID">
			<generator class="assigned" />
		</id>
		<!--<version 
			name="ReplicationVersion" 
			column="REPLICATION_VERSION"
		/>-->
		<property name="Name" column="TRANSITION_NAME" />
		<property name="Description" column="TRANSITION_DESCRIPTION" />
		
		<many-to-one name="From" column="TRANSITION_FROM" class="SkyMap.Net.Workflow.XPDL.Actdef,SkyMap.Net.Workflow" not-null="true" cascade="none" />
		<many-to-one name="To" column="TRANSITION_TO" class="SkyMap.Net.Workflow.XPDL.Actdef,SkyMap.Net.Workflow" not-null="true" cascade="none" />
		<!--<many-to-one name="Condition" column="CONDITION_ID" class="SkyMap.Net.Workflow.XPDL.Condition,SkyMap.Net.Workflow" />-->
		<bag name="Conditions" table="WF_TRANSITION_CONDITION" inverse="false" lazy="false"
			cascade="none" generic="true"  outer-join="true" fetch="subselect">
			<key column="TRANSITION_ID" />
			<many-to-many class="SkyMap.Net.Workflow.XPDL.Condition,SkyMap.Net.Workflow"
				column="CONDITION_ID" />
		</bag>
	</class>
	
</hibernate-mapping>

