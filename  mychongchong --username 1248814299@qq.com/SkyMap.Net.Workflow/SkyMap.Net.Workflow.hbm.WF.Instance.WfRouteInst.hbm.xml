<?xml version="1.0" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" default-lazy="false">
	<class name="SkyMap.Net.Workflow.Instance.WfRouteInst,SkyMap.Net.Workflow" table="WF_ROUTEINST">
		<id name="Id" type="String" column="ROUTEINST_ID">
			<generator class="assigned" />
		</id>
		<!--<version 
			name="ReplicationVersion" 
			column="REPLICATION_VERSION"
		/>-->
		<property name="ProinstId" column="PROINST_ID" />
		<property name="TimeStamp" column="ROUTEINST_TIMESTAMP" />
		<many-to-one name="From" column="ROUTEINST_FROM" 
			class="SkyMap.Net.Workflow.Instance.Actinst,SkyMap.Net.Workflow" cascade="none"/>
		<many-to-one name="To" column="ROUTEINST_TO" 
			class="SkyMap.Net.Workflow.Instance.Actinst,SkyMap.Net.Workflow" cascade="none" />
	</class>
	
</hibernate-mapping>